{
	"scripts": {
		"configure": "elm-ts-interop --entrypoint Main --output src/Main/index.d.ts",
		"build": "npm run configure && parcel build index.html 404.html",
		"autogen": "ts-node ts/generate-encrypted $PASSWORD_HASH ./ts/encrypted.ts",
		"start": "parcel index.html --public-url /sarsonwedding",
		"test": "npm run configure && tsc --noEmit -p . && ava"
	},
	"devDependencies": {
		"@incrementalelm/elm-ts-interop-pro": "0.0.11",
		"@parcel/transformer-elm": "2.0.0-beta.2",
		"@parcel/transformer-image": "2.0.0-beta.2",
		"@parcel/transformer-raw": "2.0.0-beta.2",
		"@parcel/transformer-sass": "2.0.0-beta.2",
		"@types/marked": "1.2.2",
		"@types/node": "14.14.25",
		"ava": "3.15.0",
		"cssnano": "4.1.10",
		"elm": "0.19.1-5",
		"elm-hot": "1.1.6",
		"parcel": "2.0.0-beta.2",
		"regenerator-runtime": "0.13.7",
		"sass": "1.32.6",
		"ts-node": "9.1.1",
		"typescript": "4.1.3"
	},
	"dependencies": {
		"fp-ts": "2.9.5",
		"io-ts": "2.2.14"
	},
	"browserslist": "last 2 Chrome versions",
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
}
